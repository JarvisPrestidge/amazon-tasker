{
  "name": "amazon-tasker",
  "version": "1.0.0",
  "main": "dist/tasker.js",
  "author": "Jarvis Prestidge & Ryan Taylor",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "npm run build",
    "clean": "npm run clean-build && npm run clean-marko",
    "clean-build": "rm -rf dist static .cache",
    "clean-marko": "find . -type f -name '*.marko.js' -delete",
    "build": "npm run clean && tsc -p .",
    "start": "node dist/src/index.js"
  },
  "dependencies": {
    "csv-parse": "^1.2.3",
    "dts-gen": "^0.5.6",
    "koa": "^2.3.0",
    "koa-body": "^2.4.0",
    "koa-router": "^7.2.1",
    "koa-static": "^4.0.1",
    "lasso": "^2.11.16",
    "lasso-marko": "^2.3.0",
    "marko": "^4.5.0-beta.4",
    "nodemailer": "^4.1.0",
    "opn": "^5.1.0",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.4"
  },
  "devDependencies": {
    "@types/csv-parse": "^1.1.10",
    "@types/koa": "^2.0.39",
    "@types/koa-router": "^7.0.23",
    "@types/koa-static": "^3.0.1",
    "@types/node": "^8.0.28",
    "@types/nodemailer": "^3.1.3",
    "@types/opn": "^3.0.28",
    "@types/request": "^2.0.3",
    "@types/request-promise-native": "^1.0.6",
    "copyfiles": "^1.2.0",
    "tslint": "^5.7.0",
    "typescript": "^2.5.2"
  }
}
